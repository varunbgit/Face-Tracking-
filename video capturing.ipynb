   "source": [
    "import cv2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "2fc5ce4a",
   "metadata": {},
   "outputs": [],
   "source": [
    "cam=cv2.VideoCapture(0)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "1a9a112d",
   "metadata": {},
   "outputs": [],
   "source": [
    "while True:\n",
    "    ret,img=cam.read()\n",
    "    cv2.imshow(\"window\",img)\n",
    "    key=cv2.waitKey(1)\n",
    "    if key==13:\n",
    "        break\n",
    "cv2.destroyAllWindows()\n",
    "cam.release()"
   ]
  },
